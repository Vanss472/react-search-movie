{
  "name": "gatsby-typescript-styled-components-boilerplate",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Vanessa Martinez",
  "dependencies": {
    "axios": "^0.21.1",
    "babel-plugin-styled-components": "^1.10.7",
    "clean-dir": "^1.0.3",
    "dotenv": "^8.2.0",
    "gatsby": "^2.21.30",
    "gatsby-cli": "^2.19.2",
    "gatsby-image": "^2.4.3",
    "gatsby-plugin-manifest": "^2.4.3",
    "gatsby-plugin-offline": "^3.2.2",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sharp": "^2.6.3",
    "gatsby-plugin-styled-components": "^3.3.1",
    "gatsby-source-filesystem": "^2.3.3",
    "gatsby-transformer-sharp": "^2.5.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "styled-components": "^5.1.0",
    "styled-normalize": "^8.0.7",
    "what-input": "^5.2.9"
  },
  "devDependencies": {
    "@types/node": "^14.0.1",
    "@types/reach__router": "^1.3.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/react-redux": "^7.1.9",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "prettier": "2.0.5",
    "redux-devtools": "^3.5.0",
    "typescript": "^3.9.2"
  },
  "keywords": [
    "gatsby",
    "react",
    "styled-component",
    "frontend",
    "javascript",
    "typescript"
  ],
  "browserslist": [
    "defaults",
    "not ie <= 10"
  ],
  "license": "MIT",
  "scripts": {
    "start": "npm run develop",
    "develop": "cleandir ./.cache && gatsby develop",
    "build": "cleandir ./.cache && gatsby build",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ignore-path .gitignore --ext .ts,.tsx,.js",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  }
}
